{
	"$schema": "https://biomejs.dev/schemas/1.9.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noConsole": {
					"level": "error"
				}
			}
		}
	},
	"formatter": {
		"enabled": true
	},
	"files": {
		"ignore": ["dist", "cdk.out"]
	}
}
